{
  "name": "@kiyo-e/claude_code_proxy",
  "version": "0.1.0",
  "module": "dist/cli.js",
  "type": "module",
  "bin": {
    "claude-code-proxy": "./dist/cli.js"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "start": "bun run --hot src/index.ts",
    "deploy": "wrangler deploy --minify src/index.ts",
    "build": "bun build src/index.ts --outfile dist/cli.js --target=node --minify",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250601.0",
    "bun-types": "latest",
    "wrangler": "^4.18.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "hono": "^4.7.11"
  }
}