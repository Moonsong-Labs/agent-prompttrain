{
  "name": "claude-nexus-proxy",
  "version": "0.2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/moonsong-labs/claude-nexus-proxy.git"
  },
  "bin": "./bin",
  "files": [
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bin": "./bin",
    "dev": "wrangler dev src/index.ts",
    "start": "bun run --hot src/index.ts",
    "deploy": "wrangler deploy --minify src/index.ts",
    "build": "bun build ./src/server.ts --outfile=./bin --target=node --format=esm --banner='#!/usr/bin/env node'",
    "prepack": "bun run build && chmod +x ./bin"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250601.0",
    "bun-types": "latest",
    "wrangler": "^4.18.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.14.3",
    "@slack/webhook": "^7.0.5",
    "axios": "^1.7.2",
    "dotenv": "^16.5.0",
    "hono": "^4.7.11"
  }
}
