{
  "name": "@claude-nexus/proxy",
  "version": "2.0.0",
  "description": "Claude Nexus Proxy Service - API proxy for Claude with telemetry and multi-subscription support",
  "private": true,
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "claude-nexus-proxy": "./dist/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moonsong-labs/claude-nexus-proxy.git",
    "directory": "services/proxy"
  },
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "dev": "bun run src/main.ts",
    "dev:watch": "bun --watch src/main.ts",
    "build": "bun scripts/build-production.ts",
    "start": "bun dist/main.js",
    "start:prod": "NODE_ENV=production bun dist/main.js",
    "typecheck": "tsc --build",
    "lint": "eslint src/**/*.ts",
    "test": "bun test",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@claude-nexus/shared": "workspace:*",
    "@hono/node-server": "^1.13.7",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@octokit/rest": "^22.0.0",
    "@slack/webhook": "^7.0.3",
    "gray-matter": "^4.0.3",
    "hono": "^4.6.13",
    "js-yaml": "^4.1.0",
    "lru-cache": "^11.1.0",
    "nanoid": "^5.0.8",
    "node-cache": "^5.1.2",
    "p-limit": "^6.1.0",
    "zod": "^3.24.1",
    "redact-pii": "^3.4.0",
    "rate-limiter-flexible": "^5.0.3"
  },
  "devDependencies": {
    "@types/bun": "^1.2.17",
    "@types/node": "^24.0.10",
    "@types/js-yaml": "^4.0.9"
  },
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.0.0"
  }
}
